<Window
    x:Class="AssemblyResources.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AssemblyResources"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="MainWindow"
    Width="300"
    Height="300"
    mc:Ignorable="d">
    <StackPanel>
        <Button
            Name="btnPlay"
            Margin="5"
            Padding="5"
            Click="btnPlay_Click"
            Content="Play" />
        <Image
            Name="imgWrapper"
            Margin="5"
            Source="./Resource/Images/Blue hills.jpg" />
        <MediaElement
            Name="soundWrapper"
            LoadedBehavior="Manual"
            Source="./Resource/Sounds/start.wav" />
    </StackPanel>
</Window>
